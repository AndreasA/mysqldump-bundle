{
    "name": "vyfony/mysqldump-bundle",
    "type": "symfony-bundle",
    "description": "Symfony VyfonyMysqldumpBundle",
    "keywords": ["mysqldump", "mysql", "dump", "symfony-bundle"],
    "homepage": "https://github.com/vyfony/mysqldump-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Dyshkant",
            "email": "vyshkant@gmail.com",
            "homepage": "https://vyshkant.github.io"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "symfony/console": "^4.0",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/process": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phan/phan": "^0.12.0",
        "phpro/grumphp": "^0.14.1",
        "phpunit/phpunit": "^7.3",
        "povils/phpmnd": "^2.0",
        "sensiolabs/security-checker": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Vyfony\\Bundle\\MysqldumpBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyfony\\Bundle\\MysqldumpBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
